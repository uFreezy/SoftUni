import java.util.Arrays;
import java.util.Scanner;


public class SortArrayOfNumbers {

	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		int n = input.nextInt();
		input.nextLine();
		int[] arr = new int[n];
		for(int ele : arr) {
			ele = input.nextInt();
			//input.nextLine();
		}
		Arrays.sort(arr);
		for(int ele : arr) {
			System.out.printf("%s ",ele);
		}
	}

}
