import java.util.Scanner;


public class CountOfBitsOne {

	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		int a = input.nextInt();
		int counter = 0;
		
		for(int i = 0; i < Integer.toBinaryString(a).length(); i++){
			
			if((0 | i) == 1) {
				i = i >> 1;
				counter++;
				System.out.println(counter);
			}
		}

	}

}
