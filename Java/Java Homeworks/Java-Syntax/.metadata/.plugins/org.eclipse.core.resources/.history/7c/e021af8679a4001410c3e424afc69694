import java.util.Scanner;


public class CountOfBitsOne {

	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		int a = input.nextInt();
		int counter = 0;
		
		System.out.println(Integer.toBinaryString(a));
		for(int i = 0; i < 3; i++){
			
			if((0 | i) == 1) {
				counter++;
				System.out.println(counter);
			}
			a = a >> 1;
			System.out.println(Integer.toBinaryString(a));
		}

	}

}
