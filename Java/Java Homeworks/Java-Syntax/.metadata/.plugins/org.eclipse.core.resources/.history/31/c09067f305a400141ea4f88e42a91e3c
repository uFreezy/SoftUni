import java.util.Scanner;


public class SmallestOutOf3Numbs {

	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		int a = input.nextInt();
		int b = input.nextInt();
		int c = input.nextInt();
		
		if(a < b) {
			if(a < c) {
				System.out.println(a);
			}
			else {
				System.out.println(c);
			}
		}
		else if(b < a) {
			if(b < c) {
				System.out.println(b);
			}
			else {
				System.out.println(c);
			}
		}
		else if(c < a) {
			if(c < b) {
				System.out.println(c);
			}
			else {
				System.out.println(b);
			}
		}

	}

}
